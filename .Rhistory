OECD <- vroom("data/DATA OCDE.csv")
WDI <- vroom("data/DATA WDI.csv")
OECD <- OECD |>
select(`Reference area`, Measure, TIME_PERIOD, OBS_VALUE) |>
rename(
country = `Reference area`,
measure = Measure,
time = TIME_PERIOD,
value = OBS_VALUE
) |>
mutate(
time = as.numeric(time),
value = as.numeric(value)
) |>
drop_na()
WDI <- WDI |>
select(-c(`Time Code`,`Country Code`)) |>
pivot_longer(
cols = -c(`Time`,`Country Name`),
names_to = "measure",
values_to = "value"
) |>
rename(
country = `Country Name`,
time = Time
) |>
mutate(
time = as.numeric(time),
value = as.numeric(ifelse(value == "..", NA, value))
) |>
drop_na()
data <- full_join(
OECD, WDI,
by = c("country", "measure", "value", "time")
) |>
drop_na()
data |>
summarise(
"Countries" = n_distinct(country),
"Variables" = n_distinct(measure),
"Start date" = min(time),
"End date" = max(time)
) |>
kable()
data |>
pivot_wider()
data |>
pivot_wider(
cols = -c("country","time")
)
data |>
pivot_wider(
cols = -c("country","time"),
names_to = "measure",
values_to = "value"
)
data |>
pivot_wider(
cols = -c("country","time"),
names_from = "measure",
values_from = "value"
)
data |>
pivot_wider(
names_from = "measure",
values_from = "value"
)
data <- data |>
pivot_wider(
names_from = "measure",
values_from = "value"
)
View(data)
data |>
pivot_wider(
names_from = "measure",
values_from = "value"
) |>
drop_na()
